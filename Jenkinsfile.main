pipeline {
	agent any
	stages {
		stage('CreateDockerImage') {
			steps {
			// builds docker image using repo's dockerfile & pushes it to docker repo
				script {
					sh "docker build -t miloshsh/spring-petclinic:latest ."
					sh "docker push miloshsh/spring-petclinic:latest"
				}
			}
		}
	}
}
